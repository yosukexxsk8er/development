{
  "name": "Tab history",
  "description": "Tab history",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab", "tabs", "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "Show the popup"
    }
  }
}
